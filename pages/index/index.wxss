/**index.wxss**/

page {
  background-color: #f5f5f5;
}

.index-page {
  height: 100vh;
}

.navbar {
  /* position: fixed;
  top: 0;
  left: 0;
  z-index: 999; */
  display: flex;
  background-color: #fff;
  width: 100%;
  height: 84rpx;
}

.navbar label {
  display: block;
  height: 84rpx;
  text-align: center;
  flex: 1;
  font-size: 32rpx;
  position: relative;
  line-height: 84rpx;
  color: #000;
}

.navbar label::after {
  content: "";
  display: inline-block;
  height: 6rpx;
  background-color: transparent;
  position: absolute;
  left: 70rpx;
  right: 70rpx;
  bottom: 8rpx;
}

.navbar .cur text {
  background-color: #34b36c;
  color: #fff;
  padding: 10rpx 20rpx;
  border-radius: 50px;
}

.navbar radio {
  display: none;
}

/* .tab {
  padding-top: 84rpx;
} */

.video-body {
  position: relative;
  background-color: #fff;
}

.video-bg {
  width: 100%;
  height: auto;
}

.video-box {
  position: absolute;
  top: 14rpx;
  left: 50%;
  margin-left: -360rpx;
  z-index: 99;
}

video, .video-masker {
  width: 720rpx;
  height: 360rpx;
}

.video-masker {
  background-color: rgba(0, 0, 0, 0.8);
}

.video-masker image {
  width: 128rpx;
  height: 128rpx;
}

.column-body {
  margin-bottom: 20rpx;
  background-color: #fff;
  padding: 0 40rpx;
}

.column-title {
  font-size: 34rpx;
  /* color: #4d4d4d;
  display: flex;
  justify-content: center;
  align-items: center;
  padding: 30rpx 0 20rpx;
  border-bottom: 2rpx solid #ebebeb;
  margin-bottom: 20rpx; */
  display: table;
  white-space: nowrap;
  height: auto;
  overflow: hidden;
  line-height: 1;
  text-align: center;
  padding: 10px 0;
  color: #666;
}

.column-title:after, .column-title:before {
  content: '';
  display: table-cell;
  position: relative;
  top: 50%;
  width: 50%;
  background-repeat: no-repeat;
  background-image: url(data:image/png;base64,iVBORw0KGgoAAAANSUhEUgAABaAAAAACCAYAAACuTHuKAAAAGXRFWâ€¦GBJQby/mUcJn5sJXQmOQMAAAAAAJqt+2prAAAAAACg2xdgANk6BEVuJgyMAAAAAElFTkSuQmCC);
}

.column-title:before {
    background-position: right 1em top 50%;
}

.column-title:after {
    background-position: left 1em top 50%;
}

.column-main {
  display: flex;
  justify-content: space-between;
  flex-wrap: wrap;
}

.title-icon {
  width: 36rpx;
  height: 36rpx;
  background: url("data:image/png;base64,") no-repeat center;
  background-size: cover;
  margin-right: 14rpx;
}

.service-body {
  padding-bottom: 16rpx;
  padding: 0 20rpx;
}

.service-box {
  display: flex;
}

.service-item {
  display: flex;
  flex-direction: column;
  align-items: center;
  font-size: 30rpx;
  flex: 1;
}

.service-title {
  padding-top: 0;
  padding-bottom: 28rpx;
  border-bottom: none;
  margin-bottom: 0;
}

.service-item image {
  width: 120rpx;
  height: 120rpx;
}

.service-item text {
  padding: 16rpx 0;
  font-size: 30rpx;
  color: #5e5e5e;
}

.hot-service-item {
  width: 316rpx;
  margin-bottom: 32rpx;
}

.hot-service-item image {
  width: 100%;
  height: 244rpx;
}

.hot-service-item .title {
  font-size: 30rpx;
  padding: 10rpx 0 5rpx;
}

.hot-service-item .desc {
  font-size: 26rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  padding: 5rpx 0;
}

.hot-service-item .price {
  font-size: 26rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  padding: 5rpx 0;
}

.hot-service-item .price text {
  font-size: 32rpx;
  color: #eb6100;
}

.column-main .read-more {
  color: #999;
  border: 1px solid #999;
  border-radius: 50rpx;
  font-size: 26rpx;
  margin: 40rpx auto 64rpx;
}

.button-hover {
  background-color: rgba(0, 0, 0, 0.1);
  opacity: 0.7;
}

.hot-product-item {
  width: 320rpx;
  margin-bottom: 32rpx;
}

.hot-product-item image {
  width: 100%;
  height: 280rpx;
}

.hot-product-item .title {
  font-size: 30rpx;
  padding: 10rpx 0 5rpx;
}

.hot-product-item .desc {
  font-size: 26rpx;
  padding: 5rpx 0;
  color: #999;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
}

.quality-body {
  flex-wrap: nowrap;
  padding: 30rpx 20rpx;
}

.quality-item {
  text-align: center;
}

.quality-item image {
  width: 50rpx;
  height: 50rpx;
}

.quality-item .title {
  font-size: 24rpx;
  padding: 10rpx 0 5rpx;
}

.quality-item .desc {
  font-size: 20rpx;
  padding: 5rpx 0;
  color: #999;
}

.about-body {
  display: flex;
  justify-content: space-between;
  padding: 20rpx;
}

.about-body navigator {
  display: block;
  width: 348rpx;
  height: 166rpx;
}

.about-body navigator image {
  width: 100%;
  height: auto;
}

.shop-join-body {
  padding: 0 40rpx 20rpx;
}

.shop-join-body .column-main {
}

.shop-join-body navigator {
  width: 100%;
  margin-bottom: 20rpx;
}

.shop-join-body image {
  width: 100%;
  height: auto;
}

.project-item {
  border-radius: 8rpx;
  overflow: hidden;
  position: relative;
  width: 326rpx;
  height: 224rpx;
  margin-bottom: 20rpx;
}

.project-item image {
  width: 100%;
  height: 100%;
  background-color: #fdafc5;
}

.project-item .title {
  position: absolute;
  left: 0;
  bottom: 0;
  width: 100%;
  background-color: rgba(0, 0, 0, 0.5);
  color: #fff;
  text-align: center;
  line-height: 64rpx;
  height: 64rpx;
  font-size: 30rpx;
}

.skill-item {
  display: flex;
  width: 100%;
  margin-bottom: 12rpx;
}

.skill-item .title {
  margin-bottom: 20rpx;
}

.skill-item .title text {
  color: #fff;
  padding: 10rpx 20rpx;
  border-radius: 50rpx;
}

.skill-item:nth-child(1) .title text {
  background-color: #ff7575;
}

.skill-item:nth-child(2) .title text {
  background-color: #efc152;
}

.skill-item:nth-child(3) .title text {
  background-color: #35b36c;
}

.skill-body image {
  width: 220rpx;
  height: 220rpx;
  background-color: gray;
  flex-shrink: 0;
}

.skill-body  .content {
  padding: 20rpx 30rpx 20rpx 20rpx;
  flex: 1;
}

.skill-body .title {
  font-size: 30rpx;
  padding-bottom: 10rpx;
}

.skill-body .desc {
  font-size: 24rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 4;
  -webkit-box-orient: vertical;
}

.job-body navigator {
  display: block;
  width: 670rpx;
  height: 250rpx;
}

.job-body image {
  width: 670rpx;
  height: 250rpx;
}

.coupon-box {
  position: fixed;
  left: 0;
  top: 0;
  z-index: 9999;
}

.contact-body {
  font-size: 0;
  margin-bottom: 20rpx;
}

.contact-body image {
  width: 100%;
}
